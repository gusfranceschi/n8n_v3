cat_list.txt\ntouch concat_list.txt\n\n# Adiciona cada vídeo à lista\n${videoPaths.map(path => `echo \"file '${path}'\" >> concat_list.txt`).join('\\n')}\n\n# Concatena os vídeos em um único arquivo\nffmpeg -y -f concat -safe 0 -i concat_list.txt -c copy ${outputPath}\n\necho \"Vídeo finalizado: ${outputPath}\"\n`;\n\n  return [\n    {\n      json: {\n        ffmpegScript,\n        videoPaths,\n        outputPath,\n      },\n    },\n  ];\n}\n\n// Executa a função com todos os itens de entrada\nreturn concatVideos($input.all());\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[200,720],"id":"94eb7da5-574b-40b6-af17-032128fd81be","name":"Gerar Script de Concatenação1"},{"parameters":{"command":"={{ $json.ffmpegScript }}"},"type":"n8n-nodes-base.ssh","typeVersion":1,"position":[420,720],"id":"d86b9e98-06a2-428c-9775-56a38d2ba50b","name":"Executar FFmpeg para Concatenar1","credentials":{"sshPassword":{"id":"RLwqNZIVce6BqZ7h","name":"SSH Password account"}}},{"parameters":{"options":{}},"type":"n8n-nodes-base.dateTime","typeVersion":2,"position":[1080,720],"id":"53b3cafe-c245-4931-9bfb-5602c87ad97d","name":"Obter Data Atual1"},{"parameters":{"jsCode":"// 1) pega a data/hora atual\nconst now = new Date();\n\n// 2) adiciona 2 dias\nnow.setDate(now.getDate() + 2);\n\n// 3) formata como \"YYYY-MM-DD\"\nconst formatted = now.toISOString().split('T')[0];\n\n// 4) devolve um único item com a nova data\nreturn [\n  {\n    json: {\n      datePlusOne: formatted\n    }\n  }\n];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[1300,720],"id":"d71be920-1a6f-4ac9-a8ef-5cf3b28b7e8e","name":"Calcular Data de Publicação1"},{"parameters":{"assignments":{"assignments":[{"id":"8dcb9efd-77f8-4044-a774-8d9a92767cf8","name":"output[0].seo","value":"={{ $item(\"0\").$node[\"Consolidar Dados do Cartão1\"].json[\"output\"][\"0\"][\"seo\"] }}","type":"string"}]},"options":{}},"type":"n8n-nodes-base.set","typeVersion":3.4,"position":[1520,720],"id":"c522905a-dcfc-4dc3-b4ba-4